{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50
      },  
      "description": {
        "type": "string"
      },
      "url": {
        "type": "string",
        "format": "uri"
      },
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 20  
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "description",
      "url",
      "title"
    ]
  }
  